n        guild.invites.fetch().then(invites => {\n            const user = member; // O usu√°rio que acabou de entrar\n\n            // Encontrar o convite usado pelo novo membro\n            const usedInvite = invites.find(invite => invite.uses > invite.maxUses - 1);\n\n            if (usedInvite) {\n                // Verifica se o criador do convite existe\n                const creator = usedInvite.inviter ? usedInvite.inviter : { username: 'Indefinido' };\n\n                // N√∫mero de utiliza√ß√µes do convite\n                const totalUses = usedInvite.uses || 0;\n\n                // Log para verificar a funcionalidade\n                console.log(`${user.user.username} entrou usando o convite ${usedInvite.code}, criado por ${creator.username}. O convite j√° foi usado ${totalUses} vezes.`);\n\n                // Enviar mensagem no canal\n                const channel = guild.channels.cache.get('1297770356890796032'); // Substitua pelo ID do canal onde a mensagem ser√° enviada\n                if (channel) {\n                    channel.send(`${user.user.username} entrou usando o convite ${usedInvite.code}, criado por ${creator.username}. Esse convite j√° foi usado ${totalUses} vezes.`);\n                }\n\n                // Armazena o n√∫mero de utiliza√ß√µes do convite em uma vari√°vel tempor√°ria (opcional)\n                tempVars(\"invite_uses\", totalUses);\n            } else {\n                console.log(\"Nenhum convite usado foi encontrado.\");\n            }\n        }).catch(err => {\n            console.log(\"Erro ao buscar os convites:\", err);\n        });\n    }\n}\n","storage":"0","varName":"","descriptioncolor":"#ffffff","description":"","descriptionx":false,"name":"Run Script MOD"}]}]